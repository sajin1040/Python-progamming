def fact(n):
    if n==1:
        return 1 
    if n<0:
        print("factorial doesnt exist")
    else:
        return n*fact(n-1) 
a=int(input("enter a number for which factorial has to be found: ")) 
print(fact(a)) 
def print_factors(x):
   print("The factors of",x,"are:")
   for i in range(1, x + 1):
       if x % i == 0:
           print(i) 
print(print_factors(a))
